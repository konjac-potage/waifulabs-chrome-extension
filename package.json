{
  "name": "waifu-instrumentality-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "private": true,
  "engines": {
    "node": ">=0.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@types/chrome": "0.0.88",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.2",
    "prettier": "^1.18.2",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6"
  },
  "scripts": {
    "build": "rm -rf dist/* && webpack -p",
    "start": "webpack --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,scss,js,json,md,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {}
}
